<h1>Plans</h1><br>
<table class="table table-striped table-bordered">
	<tr>
		<th>Plan Id</th>
		<th>Name</th>
		<th>Price</th>
		<th>Plan Details</th>
	</tr>
	<% @plans.each do |plan| %>
		<tr>
			<td><%= plan.id %></td>
			<td><%= plan.name %></td>
			<td><%= plan.price %></td>
			<td><%= plan.plan_details %></td>
			<td><%= link_to 'Edit', edit_plan_path(plan) %></td>
			<td><%= link_to 'Destroy', plan ,method: :delete, data: {confirm: 'Are you sure'} %></td>
		</tr>
	<% end %>
</table> 