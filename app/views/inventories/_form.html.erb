<%= bootstrap_form_for(@inventory,layout: :horizontal,:html => { :multipart => true },inline_errors: true )do |f| %>
    <%= f.text_field :inventory_friendly_id %>
      <%= f.select :status, ["assigned","unassigned"], :prompt => "Change Status"%> 
      <%= f.collection_select :customer_id, Customer.all, :id,:first_name,:prompt => "Select Customer "%>
    <%= f.primary %>
<% end %>